name keys_pet_business
offset 167
key_width 1
key_height 3
screen_width 80

; key <column> <row> <type> 
key  7 0 3 ; 2
key 16 0 3 ; 5
key 25 0 3 ; 8
key 37 0 3 ; -
key 54 0 3 ; keypad 8
key 43 0 3 ; cursor left/right
key  0 0 s ; (both shifts)
key 37 3 3 ; .

key  4 0 3 ; 1
key 13 0 3 ; 4
key 22 0 3 ; 7
key 31 0 3 ; 0
key 51 0 3 ; keypad 7
key 40 0 3 ; arrow up
key  0 0 s
key 57 0 3 ; keypad 9

key  0 2 3 ; escape
key  9 2 3 ; s
key 15 2 3 ; f
key 21 2 3 ; h
key 39 2 3 ; ]
key 27 2 3 ; k
key 33 2 3 ; ;
key 54 1 3 ; keypad 5

key  6 2 3 ; a
key 12 2 3 ; d
key 18 2 3 ; g
key 24 2 3 ; j
key 42 2 n ; return
key 30 2 3 ; l
key 36 2 3 ; @
key 57 1 3 ; keypad 6

key  1 1 4 ; tab
key  8 1 3 ; w
key 14 1 3 ; r
key 20 1 3 ; y
key 38 1 3 ; \
key 26 1 3 ; i
key 32 1 3 ; p
key 44 1 3 ; insert/delete

key  5 1 3 ; q
key 11 1 3 ; e
key 17 1 3 ; t
key 23 1 3 ; u
key 41 1 3 ; cursor up/down
key 29 1 3 ; o
key 35 1 3 ; [
key 51 1 3 ; keypad 4

key  3 3 4 ; left shift
key 13 3 3 ; c
key 19 3 3 ; b
key 31 3 3 ; .
key 57 3 3 ; keypad .
key  0 0 s ; (left shift + tab + i)
key 37 3 4 ; right shift
key 57 2 3 ; keypad 3

key  7 3 3 ; z
key 16 3 3 ; v
key 22 3 3 ; n
key 28 3 3 ; ,
key 51 3 6 ; keypad 0
key  0 0 s ; (z + a + l)
key 41 3 3 ; repeat
key 54 2 3 ; keypad 2

key  0 3 3 ; reverse
key 10 3 3 ; x
key  8 4 space_27 ; space
key 25 3 3 ; m
key 44 3 3 ; clear/home
key  0 0 s ; (reverse + a + l)
key 34 3 3 ; /
key 51 2 3 ; keypad 1

key  1 0 3 ; arrow left
key 10 0 3 ; 3
key 19 0 3 ; 6
key 28 0 3 ; 9
key 46 0 3 ; run/stop
key 34 0 3 ; :
key  0 0 s
key  0 0 s ; (tab + arrow left + del)
