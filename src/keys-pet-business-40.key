name keys_pet_business_40
offset 80
key_width 1
key_height 1
screen_width 40

; key <column> <row> <type> 
key  5 0 40_top ; 2
key 11 0 40_top ; 5
key 17 0 40_top ; 8
key 25 0 40_top ; -
key 36 0 40_top ; keypad 8
key 29 0 40_top ; cursor left/right
key  0 0 s ; (both shifts)
key 37 3 s ; .

key  3 0 40_top ; 1
key  9 0 40_top ; 4
key 15 0 40_top ; 7
key 21 0 40_top ; 0
key 33 0 40_full ; keypad 7
key 27 0 40_top ; arrow up
key  0 0 s
key 38 0 40_top ; keypad 9

key  0 5 40_left ; escape
key  7 5 40_mid ; s
key 11 5 40_mid ; f
key 15 5 40_mid ; h
key 27 5 40_mid ; ]
key 19 5 40_mid ; k
key 23 5 40_mid ; ;
key 36 3 40_mid ; keypad 5

key  5 5 40_mid ; a
key  9 5 40_mid ; d
key 13 5 40_mid ; g
key 17 5 40_mid ; j
key 29 4 40b_ret ; return
key 21 5 40_mid ; l
key 25 5 40_mid ; @
key 38 3 40_mid ; keypad 6

key  0 3 40_left_2 ; tab
key  6 3 40_mid ; w
key 10 3 40_mid ; r
key 14 3 40_mid ; y
key 26 3 40_mid ; \
key 18 3 40_mid ; i
key 22 3 40_mid ; p
key 30 3 40_mid ; insert/delete

key  4 3 40_mid ; q
key  8 3 40_mid ; e
key 12 3 40_mid ; t
key 16 3 40_mid ; u
key 28 3 40_mid ; cursor up/down
key 20 3 40_mid ; o
key 24 3 40_mid ; [
key 31 3 40_left ; keypad 4

key  3 7 40_mid_2 ; left shift
key 10 7 40_mid ; c
key 14 7 40_mid ; b
key 22 7 40_mid ; .
key 38 7 40_mid ; keypad .
key  0 0 s ; (left shift + tab + i)
key 26 7 40_mid_2 ; right shift
key 38 5 40_left ; keypad 3

key  6 7 40_mid ; z
key 12 7 40_mid ; v
key 16 7 40_mid ; n
key 20 7 40_mid ; ,
key 33 7 40_left_3 ; keypad 0
key  0 0 s ; (z + a + l)
key 29 7 40_mid ; repeat
key 36 5 40_mid ; keypad 2

key  0 3 s ; reverse
key  8 7 40_mid ; x
key  6 9 40_space_18 ; space
key 18 7 40_mid ; m
key 31 7 40_mid ; clear/home
key  0 0 s ; (reverse + a + l)
key 24 7 40_mid ; /
key 33 5 40_left ; keypad 1

key  0 0 40_full ; arrow left
key  7 0 40_top ; 3
key 13 0 40_top ; 6
key 19 0 40_top ; 9
key 31 0 40_top ; run/stop
key 23 0 40_top ; :
key  0 0 s
key  0 0 s ; (tab + arrow left + del)
