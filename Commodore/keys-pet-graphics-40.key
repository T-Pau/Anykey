name pet_graphics_40
offset 80
key_width 1
key_height 1
screen_width 40

; key <column> <row> <type> <name>
key  3 0 40_top exclamation_mark
key  7 0 40_top hash
key 11 0 40_top percent
key 15 0 40_top ampersent
key 19 0 40_top paren_open
key 23 0 40_top left_arrow
key 31 0 40_full clear
key 36 0 40_top cursor_left_right

key  5 0 40_top double_quote
key  9 0 40_top dollar
key 13 0 40_top single_quote
key 17 0 40_top backslash
key 21 0 40_top paren_close
key  0 0 s <none>
key 34 0 40_top cursor_up_down
key 38 0 40_top insert

key  4 3 40_mid q
key  8 3 40_mid e
key 12 3 40_mid t
key 16 3 40_mid u
key 20 3 40_mid o
key 24 3 40_mid up_arrow
key 31 3 40_left 7
key 36 3 40_mid 9

key  6 3 40_mid w
key 10 3 40_mid r
key 14 3 40_mid y
key 18 3 40_mid i
key 22 3 40_mid p
key  0 0 s <none>
key 34 3 40_mid 8
key 38 3 40_mid slash

key  5 5 40_mid a
key  9 5 40_mid d
key 13 5 40_mid g
key 17 5 40_mid j
key 21 5 40_mid l
key  0 0 s <none>
key 31 5 40_left 4
key 36 5 40_mid 6

key  7 5 40_mid s
key 11 5 40_mid f
key 15 5 40_mid h
key 19 5 40_mid k
key 23 5 40_mid colon
key  0 0 s <none>
key 34 5 40_mid 5
key 38 5 40_mid asterisk

key  6 7 40_mid z
key 10 7 40_mid c
key 14 7 40_mid b
key 18 7 40_mid m
key 22 7 40_mid semicolon
key 27 5 40_mid_2 return
key 31 7 40_left 1
key 36 7 40_mid 3

key  8 7 40_mid x
key 12 7 40_mid v
key 16 7 40_mid n
key 20 7 40_mid comma
key 24 7 40_mid question_mark
key  0 0 s <none>
key 34 7 40_mid 2
key 38 7 40_mid plus

key  1 5 40g_shl left_shift
key  0 0 40_full at
key 27 0 40_top square_close
key  0 0 s <none>
key 28 2 40g_gt greater
key 26 7 40_mid_3 right_shift
key 31 9 40_left 0
key 36 9 40_mid minus

key  0 3 40_left_2 reverse
key 25 0 40_top square_open
key  7 9 40_space_18 space
key 26 3 40_mid less
key 25 5 40_mid stop
key 11 0 s <none>
key 34 9 40_mid period
key 38 9 40_mid equals
