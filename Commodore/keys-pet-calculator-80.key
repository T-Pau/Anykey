name pet_calculator_80
offset 160
key_width 5
key_height 3
screen_width 80

; key <column> <row> <type> <name>
key  0 0 n exclamation_mark
key  2 0 n hash
key  4 0 n percent
key  6 0 n ampersent
key  8 0 n paren_open
key 10 0 n left_arrow
key 12 0 n clear
key 14 0 n cursor_left_right

key  1 0 n double_quote
key  3 0 n dollar
key  5 0 n single_quote
key  7 0 n backslash
key  9 0 n paren_close
key  0 0 s <none>
key 13 0 n cursor_up_down
key 15 0 n insert

key  0 1 n q
key  2 1 n e
key  4 1 n t
key  6 1 n u
key  8 1 n o
key 10 1 n up_arrow
key 12 1 n 7
key 14 1 n 9

key  1 1 n w
key  3 1 n r
key  5 1 n y
key  7 1 n i
key  9 1 n p
key  0 0 s <none>
key 13 1 n 8
key 15 1 n slash

key  0 2 n a
key  2 2 n d
key  4 2 n g
key  6 2 n j
key  8 2 n l
key  0 0 s <none>
key 12 2 n 4
key 14 2 n 6

key  1 2 n s
key  3 2 n f
key  5 2 n h
key  7 2 n k
key  9 2 n colon
key  0 0 s <none>
key 13 2 n 5
key 15 2 n asterisk

key  0 3 n z
key  2 3 n c
key  4 3 n b
key  6 3 n m
key  8 3 n semicolon
key 10 2 h return
key 12 3 n 1
key 14 3 n 3

key  1 3 n x
key  3 3 n v
key  5 3 n n
key  7 3 n comma
key  9 3 n question_mark
key  0 0 s <none>
key 13 3 n 2
key 15 3 n plus

key  0 4 n left_shift
key  2 4 n at
key  4 4 n square_close
key  0 0 s <none>
key  8 4 n greater
key 10 4 n right_shift
key 12 4 n 0
key 14 4 n minus

key  1 4 n reverse
key  3 4 n square_open
key  5 4 w space
key  7 4 n less
key  9 4 n stop
key 11 0 s <none>
key 13 4 n period
key 15 4 n equals
