name pet_calculator_40

offset 83
key_width 1
key_height 1
screen_width 40

; key <column> <row> <type> <name>
key  0 0 40_full exclamation_mark
key  5 0 40_top hash
key  9 0 40_top percent
key 13 0 40_top ampersent
key 17 0 40_top paren_open
key 21 0 40_top left_arrow
key 25 0 40_full clear
key 30 0 40_top cursor_left_right

key  3 0 40_top double_quote
key  7 0 40_top dollar
key 11 0 40_top single_quote
key 15 0 40_top backslash
key 19 0 40_top paren_close
key  0 0 s <none>
key 28 0 40_top cursor_up_down
key 32 0 40_top insert

key  0 3 40_left q
key  5 3 40_mid e
key  9 3 40_mid t
key 13 3 40_mid u
key 17 3 40_mid o
key 21 3 40_mid up_arrow
key 25 3 40_left 7
key 30 3 40_mid 9

key  3 3 40_mid w
key  7 3 40_mid r
key 11 3 40_mid y
key 15 3 40_mid i
key 19 3 40_mid p
key  0 0 s <none>
key 28 3 40_mid 8
key 32 3 40_mid slash

key  0 5 40_left a
key  5 5 40_mid d
key  9 5 40_mid g
key 13 5 40_mid j
key 17 5 40_mid l
key  0 0 s <none>
key 25 5 40_left 4
key 30 5 40_mid 6

key  3 5 40_mid s
key  7 5 40_mid f
key 11 5 40_mid h
key 15 5 40_mid k
key 19 5 40_mid colon
key  0 0 s <none>
key 28 5 40_mid 5
key 32 5 40_mid asterisk

key  0 7 40_left z
key  5 7 40_mid c
key  9 7 40_mid b
key 13 7 40_mid m
key 17 7 40_mid semicolon
key 21 5 40c_ret return
key 25 7 40_left 1
key 30 7 40_mid 3

key  3 7 40_mid x
key  7 7 40_mid v
key 11 7 40_mid n
key 15 7 40_mid comma
key 19 7 40_mid question_mark
key  0 0 s <none>
key 28 7 40_mid 2
key 32 7 40_mid plus

key  0 9 40_left left_shift
key  5 9 40_mid at
key  9 9 40_mid square_close
key  0 0 s <none>
key 17 9 40_mid greater
key 21 9 40_mid right_shift
key 25 9 40_left 0
key 30 9 40_mid minus

key  3 9 40_mid reverse
key  7 9 40_mid square_open
key 11 9 40_mid_3 space
key 15 9 40_mid less
key 19 9 40_mid stop
key 11 0 s <none>
key 28 9 40_mid period
key 32 9 40_mid equals
