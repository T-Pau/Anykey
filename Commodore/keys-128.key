name keys_128
offset 80

key 30 2 2 ; insert / delete
key 28 6 4 ; return
key 30 8 2 ; cursor left / right
key 38 0 2 ; F7
key 32 0 2 ; F1
key 34 0 2 ; F3
key 36 0 2 ; F5
key 28 8 2 ; cursor up / down

key  6 2 2 ; 3
key  5 4 2 ; W
key  4 6 2 ; A
key  8 2 2 ; 4
key  5 8 2 ; Z
key  6 6 2 ; S
key  7 4 2 ; E
key  2 6 2_3 ; shift lock + left shift

key 10 2 2 ; 5
key  9 4 2 ; R
key  8 6 2 ; D
key 12 2 2 ; 6
key  9 8 2 ; C
key 10 6 2 ; F
key 11 4 2 ; T
key  7 8 2 ; X

key 14 2 2 ; 7
key 13 4 2 ; Y
key 12 6 2 ; G
key 16 2 2 ; 8
key 13 8 2 ; B
key 14 6 2 ; H
key 15 4 2 ; U
key 11 8 2 ; V

key 18 2 2 ; 9
key 17 4 2 ; I
key 16 6 2 ; J
key 20 2 2 ; 0
key 17 8 2 ; M
key 18 6 2 ; K
key 19 4 2 ; O
key 15 8 2 ; N

key 22 2 2 ; +
key 21 4 2 ; P
key 20 6 2 ; L
key 24 2 2 ; -
key 21 8 2 ; .
key 22 6 2 ; :
key 23 4 2 ; @
key 19 8 2 ; 

key 26 2 2 ; Â£
key 25 4 2 ; *
key 24 6 2 ; ;
key 28 2 2 ; clear / home
key 25 8 3 ; right shift
key 26 6 2 ; =
key 27 4 2 ; ^
key 23 8 2 ; /

key 2 2 2 ; 1
key 0 2 2 ; <-
key 0 4 3 ; control
key 4 2 2 ; 2
key 7 10 18; space
key 0 8 2 ; C=
key 3 4 2 ; Q
key 0 6 2 ; run / stop

key 12 0 2 ; help
key 34 2 2 ; keypad 8
key 34 4 2 ; keypad 5
key  2 0 2 ; tab
key 34 6 2 ; keypad 2
key 32 4 2 ; keypad 4
key 32 2 2 ; keypad 7
key 32 6 2 ; keypad 1

key  0 0 2 ; escape
key 38 2 2 ; keypad + 
key 38 4 2 ; keypad -
key 14 0 2 ; line feed
key 38 6 2_2 ; keypad enter
key 36 4 2 ; keypad 6
key 36 2 2 ; keypad 9
key 36 6 2 ; keypad 3

key  4 0 2 ; alt
key 32 8 4 ; keypad 0
key 36 8 2 ; keypad .
key 24 0 2 ; cursor up
key 26 0 2 ; cursor down
key 28 0 2 ; cursor left
key 30 0 2 ; cursor right
key 18 0 2 ; no scroll

key  6 0 2 ; caps lock
key 16 0 2 ; 40/80 display
key 29 4 3 ; restore
