name pet_business_40
offset 80
key_width 1
key_height 1
screen_width 40

; key <column> <row> <type> <name>
key  5 0 40_top 2
key 11 0 40_top 5
key 17 0 40_top 8
key 25 0 40_top minus
key 36 0 40_top keypad_8
key 29 0 40_top cursor_left_right
key  0 0 s shift
key 37 3 s period2

key  3 0 40_top 1
key  9 0 40_top 4
key 15 0 40_top 7
key 21 0 40_top 0
key 33 0 40_full keypad_7
key 27 0 40_top up_arrow
key  0 0 s none
key 38 0 40_top keypad_9

key  0 5 40_left escape
key  7 5 40_mid s
key 11 5 40_mid f
key 15 5 40_mid h
key 27 5 40_mid square_close
key 19 5 40_mid k
key 23 5 40_mid semicolon
key 36 3 40_mid keypad_5

key  5 5 40_mid a
key  9 5 40_mid d
key 13 5 40_mid g
key 17 5 40_mid j
key 29 4 40b_ret return
key 21 5 40_mid l
key 25 5 40_mid at
key 38 3 40_mid keypad_6

key  0 3 40_left_2 tab
key  6 3 40_mid w
key 10 3 40_mid r
key 14 3 40_mid y
key 26 3 40_mid backslash
key 18 3 40_mid i
key 22 3 40_mid p
key 30 3 40_mid insert

key  4 3 40_mid q
key  8 3 40_mid e
key 12 3 40_mid t
key 16 3 40_mid u
key 28 3 40_mid cursor_up_down
key 20 3 40_mid o
key 24 3 40_mid square_open
key 33 3 40_left keypad_4

key  3 5 40b_shl left_shift
key 10 7 40_mid c
key 14 7 40_mid b
key 22 7 40_mid period
key 38 7 40_mid keypad_period
key  0 0 s <none>; (left shift + tab + i)
key 26 7 40_mid_2 right_shift
key 38 5 40_mid keypad_3

key  6 7 40_mid z
key 12 7 40_mid v
key 16 7 40_mid n
key 20 7 40_mid comma
key 33 7 40_left_3 keypad_0
key  0 0 s <none>; (z + a + l)
key 29 7 40_mid repeat
key 36 5 40_mid keypad_2

key  0 3 s reverse
key  8 7 40_mid x
key  6 9 40_space_18 space
key 18 7 40_mid m
key 31 7 40_mid clear
key  0 0 s <none> ; (reverse + a + l)
key 24 7 40_mid slash
key 33 5 40_left keypad_1

key  0 0 40_full left_arrow
key  7 0 40_top 3
key 13 0 40_top 6
key 19 0 40_top 9
key 31 0 40_top run_stop
key 23 0 40_top colon
key  0 0 s <none>
key  0 0 s <none> ; (tab + arrow left + del)
