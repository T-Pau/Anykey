name keys_vic20
offset 45
key_height 2
screen_width 22

key  1 0 1 ; 1
key  0 0 1 ; <-
key  0 1 2 ; control
key  0 2 1 ; run / stop
key  3 4 10 ; space
key  1 3 1 ; C=
key  2 1 1 ; Q
key  2 0 1 ; 2

key  3 0 1 ; 3
key  3 1 1 ; W
key  2 2 1 ; A
key  1 2 left_shift ; shift lock + left shift
key  3 3 1 ; Z
key  3 2 1 ; S
key  4 1 1 ; E
key  4 0 1 ; 4

key  5 0 1 ; 5
key  5 1 1 ; R
key  4 2 1 ; D
key  4 3 1 ; X
key  5 3 1 ; C
key  5 2 1 ; F
key  6 1 1 ; T
key  6 0 1 ; 6

key  7 0 1 ; 7
key  7 1 1 ; Y
key  6 2 1 ; G
key  6 3 1 ; V
key  7 3 1 ; B
key  7 2 1 ; H
key  8 1 1 ; U
key  8 0 1 ; 8

key  9 0 1 ; 9
key  9 1 1 ; I
key  8 2 1 ; J
key  8 3 1 ; N
key  9 3 1 ; M
key  9 2 1 ; K
key 10 1 1 ; O
key 10 0 1 ; 0

key 11 0 1 ; +
key 11 1 1 ; P
key 10 2 1 ; L
key 10 3 1 ; ,
key 11 3 1 ; .
key 11 2 1 ; :
key 12 1 1 ; @
key 12 0 1 ; -

key 13 0 1 ; Â£
key 13 1 1 ; *
key 12 2 1 ; ;
key 12 3 1 ; /
key 13 3 1 ; right shift
key 13 2 1 ; =
key 14 1 1 ; ^
key 14 0 1 ; clear / home

key 15 0 1 ; insert / delete
key 14 2 2 ; return
key 15 3 1 ; cursor left / right
key 14 3 1 ; cursor up / down
key 17 0 2 ; F1
key 18 1 2 ; F3
key 17 2 2 ; F5
key 18 3 2 ; F7

key 15 1 2 ; restore
